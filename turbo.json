{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "dist/**", "build/**", "out/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  },
  "globalDependencies": [".env", "turbo.json"],
  "packageOverrides": {
    "packages/common": {
      "build": {
        "outputs": []
      }
    },
    "packages/db": {
      "build": {
        "outputs": []
      }
    },
    "packages/backend-common": {
      "build": {
        "outputs": []
      }
    }
  }
}
