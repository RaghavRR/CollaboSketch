{
  "name": "my-turborepo",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "pages:build" : "npx @cloudflare/next-on-pages"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.12",
    "@types/react": "^19.1.4",
    "prettier": "^3.5.3",
    "turbo": "^2.5.3",
    "typescript": "5.8.2",
    "vercel": "^41.7.8",
    "wrangler": "^4.15.2"
  },
  "engines": {
  "node": ">=18",
  "pnpm": "9.0.0"
},
"packageManager": "pnpm@9.0.0"

}
